{
    "name": "richardsalvaia/php-nfse",
    "type": "library",
    "description": "API para geração e comunicação da NFSe com as Prefeituras. Este é um fork do projeto lucas-simoes/php-nfse e adaptado para prefeitura de Bauru-SP",
    "keywords": ["nfse", "sped", "nfephp"],
    "homepage": "https://github.com/richardsalvaia/php-nfse",
    "license": ["Apache-2.0"],
    "authors": [
        {
            "name": "Roberto L. Machado",
            "email": "linux.rlm@gmail.com",
            "homepage": "http://www.nfephp.org",
            "role": "Developer"
        },
        {
            "name": "Comunidade NFePHP",
            "homepage": "https://github.com/nfephp-org/sped-nfse/graphs/contributors"
        },
        {
            "name": "Lucas Simões",
            "email": "lucas_development@outlook.com",
            "role": "Developer"
        },
        {
            "name": "Richard Salvaia",
            "email": "richard@2rs.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0",
        "richardsalvaia/sped-common" : "^5.0",
        "respect/validation": "^1.1"
    },
    "require-dev": {
     "squizlabs/php_codesniffer": "^2.8",
        "phpunit/phpunit": "^5.7",
        "scrutinizer/ocular": "^1.3"
    },
    "autoload": {
        "psr-4": {
          "NFePHP\\NFSe\\": "src/"
        } 
    },
    "autoload-dev": {
        "psr-4": {
            "NFePHP\\NFSe\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.*-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
